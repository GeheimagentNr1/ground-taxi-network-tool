<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<jaxb:bindings jaxb:version="3.0" jaxb:extensionBindingPrefixes="xjc"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb">

    <jaxb:globalBindings>
        <xjc:simple />
        <jaxb:serializable />

        <xjc:javaType name="java.time.LocalDate" xmlType="xs:date"
                      adapter="io.github.threetenjaxb.core.LocalDateXmlAdapter"/>
        <xjc:javaType name="java.time.OffsetDateTime" xmlType="xs:dateTime"
                      adapter="io.github.threetenjaxb.core.OffsetDateTimeXmlAdapter"/>
    </jaxb:globalBindings>

    <!-- Special kml.xsd settings. -->
    <jaxb:bindings schemaLocation="../xsd/kml.xsd" node="//xs:schema">
    </jaxb:bindings>
</jaxb:bindings>
